public class Main{
    public static void main(String[] args){
        System.out.println("|\\_/|");    // \ 출력하기 위해 \\사용
        System.out.println("|q p|   /}");
        System.out.println("( 0 )\"\"\"\\");    //"출력하기 위해 \"사용
        System.out.println("|\"^\"`    |");
        System.out.println("||_/=\\\\__|");
    }
